    "def kminmax(A,k):\n",
    "    A.sort()    \n",
    "    kmin = A[k-1]\n",
    "    kmax = A[-k]\n",
    "    return kmin,kmax\n",
    "    \n",
    "c = [2,6,3,9,8,12,18,21]\n",
    "d = 2\n",
    "\n",
    "kminmax(c,d)\n",
